Asignacion de estructuras

struct Punto {
  x: float;
  y: float;
};
//Mismo tama√±o, mismos tipos
struct Punto3D {
  x: float;
  y: float;
  z: float;
};

main() {
  Punto a;
  Punto b;
  Punto3D c;

  a.x = 1.0;
  a.y = 1.0;

  b = a;

  // c = b; // Error, tipos no coinciden.
  // a = 0; // Error, tipos no coinciden.
}